{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "posts"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "events": {
      "type": "text"
    },
    "media": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "user": {
      "plugin": "users-permissions",
      "model": "permission"
    }
  }
}
